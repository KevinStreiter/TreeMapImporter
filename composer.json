{
    "require": {
        "php-di/php-di": "^6.1",
        "selective/config": "^0.2.0",
        "slim/php-view": "^2.2",
        "slim/psr7": "^1.0",
        "slim/slim": "4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
